<h1>Login</h1>

<div class="row">
  <div class="col s12 m7">
    <div class="card">
      <div class="card-content">
        <p>
          <%= form_for(:session, url: login_path) do |f| %>
            <%= f.label :username %>
            <%= f.text_field :username %>
            <%= f.label :password %>
            <%= f.password_field :password %>
            <%= f.submit "Login" %>
          <% end %>
        </p>
      </div>
    </div>
  </div>
</div>
